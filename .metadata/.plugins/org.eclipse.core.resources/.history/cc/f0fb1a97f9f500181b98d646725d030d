package graphs;

import graphs.GraphAdjacencyList2.Graph;

public class BreadthFirstMain {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int v =5;
		Graph graph = new Graph(v);
		// create an GraphAdjacencyList2 object to access the methods
		GraphAdjacencyList2 graphAdj = new GraphAdjacencyList2(); 
		graphAdj.addEdge(graph, 100, 101);
		graphAdj.addEdge(graph, 100, 103);
		graphAdj.addEdge(graph, 101, 102);
		graphAdj.addEdge(graph, 103, 104);
		graphAdj.addEdge(graph, 103, 105);
		graphAdj.addEdge(graph, 104, 106);
		graphAdj.addEdge(graph, 105, 106);
		graphAdj.addEdge(graph, 105, 107);
		graphAdj.addEdge(graph, 106, 107);
	}

}
