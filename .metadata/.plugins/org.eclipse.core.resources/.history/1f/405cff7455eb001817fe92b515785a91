package queues;


public class QueueWithLL {
	int front ,rear ,size,capacity;
	/* 	Linked list Node.  
 	This inner class is made static so that main() can access it 
	*/
	static class QNode {
		int data;
		QNode next ;
		
		//Constructor to create a new node 
		//By default next initialized as null
		QNode(int d){
			data=d;
			next =null;
		}
	}
}
